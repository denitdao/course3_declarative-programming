<Window x:Class="lab01.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lab01"
        mc:Ignorable="d"
        Title="MainWindow" Height="250" Width="600">
    <DockPanel>
        <Label DockPanel.Dock="Top" HorizontalAlignment="Center" Content="Label"/>
        <TextBox DockPanel.Dock="Left" Text="TextBox" Width="230"/>
        <TextBox DockPanel.Dock="Right" Text="TextBox"  Width="230"/>
        <Button DockPanel.Dock="Bottom" Margin="20,130,20,30" Content="Button" Height="20"/>
    </DockPanel>
</Window>
